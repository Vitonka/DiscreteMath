---
Упростите следующую релейно-контактную схему:
---
\begin{center}
	\begin{circuitikz} \draw
		(0, 0) node[anchor=east]{}
			to[short, o-] (1, 0)	
			to[short, -] (1, 2)
			to[short, -] (2, 2)
			to[short, -] (2, 3)
			to[short, -] (3, 3)
			to[twoport, t=$x$] (4, 3)
			to[short, -] (5, 3)
			to[twoport, t=$y$] (6, 3)
			to[short, -] (7, 3)
			to[short, -] (7, 1)
			to[twoport, t=$z$] (2, 1)
			to[short, -] (2, 2)
		(1, 0)
			to[short, -] (1, -2)
			to (2, -2)
			to (2, -1)
			to[twoport, t=$\overline{x}$] (4, -1)
			to (4, -2)
			to[twoport, t=$\overline{z}$] (8, -2)
		(2, -2)
			to (2, -3)
			to[twoport, t=$y$] (4, -3)
			to (4, -2)
		(7, 2)
			to (8, 2)
			to (8, -2)
		(8, 0)
			to[short, -o] (9, 0)
	;\end{circuitikz}
\end{center}
---
\begin{center}
	\begin{circuitikz} \draw
		(0, 0) node[anchor=east]{}
			to[short, o-] (1, 0)
			to (1, 1)
			to (2, 1)
			to[twoport, t=$x$] (3, 1)
			to (4, 1)
			to[twoport, t=$\overline{y}$] (5, 1)
			to (6, 1)
			to (6, -1)
			to[twoport, t=$z$] (1, -1)
			to (1, 0)
		(6, 0)
			to (7, 0)
			to (7, 1)
			to (8, 1)
			to[twoport, t=$\overline{x}$] (9, 1)
			to (10, 1)
			to (10, -1)
			to (9, -1)
			to[twoport, t=$y$] (8, -1)
			to (7, -1)
			to (7, 0)
		(10, 0)
			to (11, 0)
			to[twoport, t=$\overline{z}$] (12, 0)
			to[short, -o] (13, 0)
		;\end{circuitikz}
\end{center}
