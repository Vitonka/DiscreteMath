\begin{center}
	\begin{circuitikz} \draw
		(0, 0) node[anchor=east]{}
			to[short, o-] (1, 0)
			to (1, 2)
			to (2, 2)
			to[twoport, t=$x$] (3, 2)
			to (4, 2)
			to[twoport, t=$\overline{y}$] (5, 2)
			to (6, 2)
			to (6, 3)
			to (7, 3)
			to[twoport, t=$x$] (8, 3)
			to (9, 3)
			to[twoport, t=$t$] (10, 3)
			to (11, 3)
			to (11, 1)
			to[twoport, t=$z$] (6, 1)
			to (6, 2)
		(11, 2)
			to (12, 2)
			to (12, -2)
		(1, 0)
			to (1, -2)
			to (2, -2)
			to[twoport, t=$\overline{x}$] (4, -2)
			to (5, -2)
			to (5, -1)
			to (6, -1)
			to[twoport, t=$\overline{y}$] (7, -1)
			to (8, -1)
			to[twoport, t=$t$] (9, -1)
			to (10, -1)
			to (10, -3)
			to (9, -3)
			to[twoport, t=$z$] (8, -3)
			to (7, -3)
			to[twoport, t=$\overline{y}$] (6, -3)
			to (5, -3)
			to (5, -2)
		(10, -2)
			to (12, -2)
		(12, 0)
			to[short, -o] (13, 0)
		;\end{circuitikz}
\end{center}