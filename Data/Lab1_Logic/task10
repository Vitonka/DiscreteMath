			to[short, o-] (2, 0)
			to[twoport, t=$x$] (3, 0)
			to (4, 0)
			to[twoport, t=$\overline{y}$] (5, 0)
			to (6, 0)
			to[twoport, t=$\overline{z}$] (7, 0)
			to [short, -o](9, 0)
